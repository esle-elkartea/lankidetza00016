                <column name="composition"/>
            </sourceColumns>
        </join>
        <join description="composition_detail_ibfk_2" foreignPath="data/item.exdm.xml">
            <sourceColumns>
                <column name="item"/>
            </sourceColumns>
        </join>
    </joins>
    <descriptionColumns/>
