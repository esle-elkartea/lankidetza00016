<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>
  <!-- =================== Navigation Rules ================= -->

  <!-- ********************************* -->
  <!-- *********** MASTER ************** -->
  <!-- ********************************* -->

  <!-- =================== Customer ========================== -->
  <navigation-rule>
    <navigation-case>
      <from-outcome>customer_form</from-outcome>
      <to-view-id>/facelet/sales/master/customer/form.faces</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>customer_list</from-outcome>
      <to-view-id>/facelet/sales/master/customer/list.faces</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>customer_search</from-outcome>
      <to-view-id>/facelet/sales/master/customer/search.faces</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>old_customer_sw</from-outcome>
      <to-view-id>/facelet/sw/customer.faces</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>old_customer_sw_search</from-outcome>
      <to-view-id>/facelet/sw/customerSearch.faces</to-view-id>
    </navigation-case>
  </navigation-rule>

   <!-- =================== Sales Invoicing ========================== -->
  <navigation-rule>
    <navigation-case>
      <from-outcome>sales_invoicing_form</from-outcome>
      <to-view-id>/facelet/sales/master/invoicing/form.faces</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>sales_invoicing_list</from-outcome>
      <to-view-id>/facelet/sales/master/invoicing/list.faces</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>sales_invoicing_search</from-outcome>
      <to-view-id>/facelet/sales/master/invoicing/search.faces</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>sales_delivery_detail_list</from-outcome>
      <to-view-id>/facelet/sales/master/invoicing/deliveryDetail/list.faces</to-view-id>
    </navigation-case>
  </navigation-rule>
  
  <!-- =================== Delivery ========================== -->
  <navigation-rule>
    <navigation-case>
      <from-outcome>delivery_form</from-outcome>
      <to-view-id>/facelet/sales/master/delivery/form.faces</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>delivery_list</from-outcome>
      <to-view-id>/facelet/sales/master/delivery/list.faces</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>delivery_search</from-outcome>
      <to-view-id>/facelet/sales/master/delivery/search.faces</to-view-id>
    </navigation-case>
  </navigation-rule>
  
  <!-- ********************************* -->
  <!-- ************ MENU *************** -->
  <!-- ********************************* -->
  
  <navigation-rule>
    <navigation-case>
      <from-outcome>customer</from-outcome>
      <to-view-id>/facelet/sales/master/customer/form.faces</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>delivery</from-outcome>
      <to-view-id>/facelet/sales/master/delivery/form.faces</to-view-id>
    </navigation-case>
   <navigation-case>
      <from-outcome>sales_invoicing</from-outcome>
      <to-view-id>/facelet/sales/master/invoicing/form.faces</to-view-id>
   </navigation-case>
   <navigation-case>
      <from-outcome>pos</from-outcome>
      <to-view-id>/facelet/sales/auxiliary/pos/form.faces</to-view-id>
   </navigation-case>
  </navigation-rule>

</faces-config>
