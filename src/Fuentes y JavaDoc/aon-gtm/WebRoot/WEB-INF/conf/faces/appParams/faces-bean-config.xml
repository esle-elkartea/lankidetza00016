<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>

	<managed-bean>
		<managed-bean-name>appParams</managed-bean-name>
		<managed-bean-class>com.code.aon.ui.config.controller.ApplicationParameterController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>defaultParameters</property-name>
			<map-entries>
				<key-class>java.lang.String</key-class>
				<value-class>java.lang.String</value-class>
				<map-entry>
					<key>AON_TAS_PRIVATE_CODE_REQUIRED</key>
					<value>false</value>
				</map-entry>
			</map-entries>
		</managed-property>
	</managed-bean>

</faces-config>